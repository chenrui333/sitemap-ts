{
  "name": "sitemap-ts",
  "version": "0.2.1",
  "packageManager": "pnpm@7.5.0",
  "description": "Sitemap generator for TypeScript projects",
  "keywords": [
    "Sitemap",
    "Typescript",
    "Generator",
    "Robots",
    "Sitemap.xml",
    "Robots.txt"
  ],
  "homepage": "https://github.com/jbaubree/sitemap-ts#readme",
  "bugs": {
    "url": "https://github.com/antfu/sitemap-ts/issues"
  },
  "license": "MIT",
  "author": "JB Aubr√©e <jb.aubree@protonmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbaubree/sitemap-ts.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "fast-glob": "3.2.11",
    "sitemap": "7.1.1",
    "xml-formatter": "2.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@antfu/ni": "0.16.3",
    "@antfu/utils": "0.5.2",
    "@babel/types": "7.18.8",
    "@types/node": "18.0.3",
    "bumpp": "8.2.1",
    "c8": "7.11.3",
    "eslint": "8.19.0",
    "esno": "0.16.3",
    "lint-staged": "^13.0.3",
    "pnpm": "7.5.0",
    "rimraf": "3.0.2",
    "simple-git-hooks": "^2.8.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "vite": "2.9.14",
    "vitest": "0.18.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ],
    "./src/*.ts": [
      "tsc --skipLibCheck --noEmit"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
